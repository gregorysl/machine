{
  "breadcrumbs.enabled": true,
  "breadcrumbs.symbolPath": "off",
  "editor.codeLens": false,
  "editor.fontFamily": "Hack NF",
  "editor.fontSize": 12,
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": false,
  "editor.minimap.maxColumn": 25,
  "editor.minimap.showSlider": "always",
  "editor.mouseWheelZoom": true,
  "editor.parameterHints.enabled": false,
  "editor.renderIndentGuides": true,
  "editor.renderWhitespace": "all",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "*.yml": "yaml"
  },
  "git.detectSubmodulesLimit": 50,
  "git.rebaseWhenSync": true,
  "terminal.explorerKind": "external",
  "terminal.external.windowsExec": "%USERPROFILE%\\AppData\\Local\\Microsoft\\WindowsApps\\wt.exe",
  "terminal.integrated.fontFamily": "Hack NF",
  "terminal.integrated.lineHeight": 1.2,
  "terminal.integrated.rendererType": "experimentalWebgl",
  "vim.disableExtension": false,
  "vim.useSystemClipboard": true,
  "window.menuBarVisibility": "toggle",
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 0,
  "workbench.activityBar.visible": true,
  "workbench.colorTheme": "Monokai Pro (Filter Octagon)",
  "workbench.iconTheme": "Monokai Pro (Filter Octagon) Icons",
  "xmlTools.splitXmlnsOnFormat": true,
  "zenMode.centerLayout": false,
  "zenMode.fullScreen": true,
  "zenMode.hideActivityBar": true,
  "zenMode.hideLineNumbers": false,
  "zenMode.hideStatusBar": false,
  "zenMode.hideTabs": true,
  "[json]": {
    "editor.maxTokenizationLineLength": 30000
  }
}